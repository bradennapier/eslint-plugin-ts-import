{
  "name": "eslint-plugin-ts-import",
  "version": "1.2.0",
  "description": "Provide Typescript import-patterns that are expected to be followed within the repo",
  "homepage": "https://github.com/bradennapier/eslint-plugin-ts-import",
  "author": "Braden Napier <bradynapier@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/bradennapier/eslint-plugin-ts-import.git"
  },
  "bugs": {
    "url": "https://github.com/bradennapier/eslint-plugin-ts-import/issues"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "clean": "rimraf lib esm",
    "commit": "npx git-cz",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "release": "semantic-release",
    "test": "echo Tests TODO"
  },
  "peerDependencies": {
    "eslint": ">= 6 < 8",
    "typescript": ">= 3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "fast-glob": "^3.2.4",
    "is-glob": "^4.0.1",
    "minimatch": "^3.0.4",
    "resolve": "^1.17.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@types/debug": "^4.1.5",
    "@types/eslint": "^7.2.0",
    "@types/is-glob": "^4.0.1",
    "@types/minimatch": "^3.0.3",
    "@types/resolve": "^1.17.1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/experimental-utils": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "gh-pages": "^3.1.0",
    "git-cz": "^4.7.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "files": [
    "lib/",
    "esm/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:types && lint-staged",
      "pre-push": "yarn lint && yarn clean && yarn build && yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
